  <Fields>
    <Field Def="u8 disableParam_NT:1">
      <DisplayName>Do you remove it from the NT version output?</DisplayName>
      <Enum>BOOL_CIRCLECROSS_TYPE</Enum>
      <Description>Parameters marked with ○ are excluded in the NT version package.</Description>
      <EditFlags>None</EditFlags>
      <Maximum>1</Maximum>
    </Field>
    <Field Def="dummy8 disableParamReserve1:7">
      <DisplayName>Reserve for package output 1</DisplayName>
      <Description>Reserve for package output 1</Description>
      <DisplayFormat>%d</DisplayFormat>
      <SortID>1101</SortID>
    </Field>
    <Field Def="dummy8 disableParamReserve2[3]">
      <DisplayName>Reserve for package output 2</DisplayName>
      <Description>Reserve for package output 2</Description>
      <DisplayFormat>%d</DisplayFormat>
      <SortID>1102</SortID>
    </Field>
    <Field Def="u8 swordArtsType">
      <DisplayName>Swashbuckler ID</DisplayName>
      <Description>To pass to the behavior script to determine which swashbuckler</Description>
      <SortID>100</SortID>
    </Field>
    <Field Def="u8 artsSpeedType">
      <DisplayName>Arts speed</DisplayName>
      <Description>Which cancellation timing do you see? 0: Normal (left hand attack) / 1: Early / 2: Slow</Description>
      <Maximum>2</Maximum>
      <SortID>200</SortID>
    </Field>
    <Field Def="s8 refStatus">
      <DisplayName>Related status</DisplayName>
      <Enum>SWORD_ARTS_REF_STATUS_TYPE</Enum>
      <Description>Which system of arts points to refer to</Description>
      <Minimum>0</Minimum>
      <Maximum>128</Maximum>
      <SortID>400</SortID>
    </Field>
    <Field Def="u8 isRefRightArts:1">
      <DisplayName>Whether to display the arts of the right hand when the left hand (holding one hand)</DisplayName>
      <Enum>BOOL_CIRCLECROSS_TYPE</Enum>
      <Description>When set to the arts of the left-handed weapon, the arts of the right-handed weapon are displayed in FE. Assumed to be used for "weapon maneuvers" etc.</Description>
      <Maximum>1</Maximum>
      <SortID>900</SortID>
    </Field>
    <Field Def="u8 isGrayoutLeftHand:1">
      <DisplayName>Whether to gray out with the left hand (holding one hand)</DisplayName>
      <Enum>BOOL_CIRCLECROSS_TYPE</Enum>
      <Description>Whether to gray out when displaying the arts name of the left hand (one-handed)</Description>
      <Maximum>1</Maximum>
      <SortID>910</SortID>
    </Field>
    <Field Def="u8 isGrayoutRightHand:1">
      <DisplayName>Whether to gray out with the right hand (holding one hand)</DisplayName>
      <Enum>BOOL_CIRCLECROSS_TYPE</Enum>
      <Description>Whether to gray out when displaying the arts name of the right hand (one-handed)</Description>
      <Maximum>1</Maximum>
      <SortID>920</SortID>
    </Field>
    <Field Def="u8 isGrayoutBothHand:1">
      <DisplayName>Will it be grayed out when holding both hands?</DisplayName>
      <Enum>BOOL_CIRCLECROSS_TYPE</Enum>
